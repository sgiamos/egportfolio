      },
      "components": [
        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            ".post_wrap": {
              "position": "absolute",
              "top": "0",
              "left": "0",
              "right": "0",
              "z-index": "5",
              "background": "linear-gradient(0deg, rgba(23, 22, 26, 0), rgba(23, 22, 26, 0.4))",
              "@media (min-width: 991px)": {
                "opacity": "0",
                "transform": "translateY(-10px)",
                "transition": "all cubic-bezier(0.4, 0, 0.2, 1) 0.4s"
              }
            },
            ".text-wrapper": {
              "padding": "70px 0",
              "& when (@card)": {
                "padding": "70px 40px",
                "padding-left": "0",
                "@media (max-width: 991px)": {
                  "padding": "40px"
                },
                "@media (max-width: 767px)": {
                  "padding": "20px"
                }
              },
              "@media (max-width: 991px)": {
                "padding": "40px 0"
              },
              "@media (max-width: 767px)": {
                "padding": "20px 0"
              }
            },
            "& when (@reverseContent)": {
              ".row": {
                "flex-direction": "row-reverse"
              },
              ".text-wrapper when (@card)": {
                "padding": "70px 40px",
                "padding-right": "0"
              }
            },
            "& when (@card)": {
              "@media (max-width: 991px)": {
                ".text-wrapper": {
                  "padding": "40px"
                }
              },
              "@media (max-width: 767px)": {
                ".text-wrapper": {
                  "padding": "20px"
                }
              }
            },
            ".content-wrapper": {
              "overflow": "hidden",
              "border-radius": "4px",
              "& when (@card)": {
                "background": "@cardColor"
              },
              "&:hover": {
                ".post_wrap": {
                  "opacity": "1",
                  "background": "linear-gradient(0deg, rgba(23, 22, 26, 0), rgba(23, 22, 26, 0.4))",
                  "transform": "translateY(0)"
                },
                ".image-wrapper > img, .image-wrapper > a img": {
                  "transform": "scale3d(1.2, 1.2, 1)"
                },
                ".item-title a": {
                  "color": "@secondaryColor !important"
                }
              }
            },
            ".image-wrapper img": {
              "width": "100%",
              "object-fit": "cover"
            },
            ".image-wrapper, img": {
              "overflow": "hidden",
              "position": "relative",
              "border-radius": "4px",
              "transition": "all cubic-bezier(0.4, 0, 0.2, 1) 0.4s"
            },
